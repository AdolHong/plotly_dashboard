{
    "query": {
        "type": "SQL",
        "code": "SELECT * FROM sales WHERE region = '${region}' AND category IN (${categories}) AND price >= ${min_price} AND product_id IN (${product_ids}) AND order_date >= '${order_date}' LIMIT 10"
    },
    "parameters":[
        {
            "name": "region",
            "type": "single_select",
            "choices": ["华东","华南","华北","西北","西南"],
            "default": "华东",
            "sep": ",",
            "wrapper": "'"
        },
        {
            "name": "categories",
            "type": "multi_select",
            "choices": ["电子产品","服装","食品","家居","图书"],
            "default": ["电子产品"],
            "sep": ",",
            "wrapper": "'"
        },
        {
            "name": "order_date",
            "type": "date_picker",
            "format": "yyyy-MM-dd",
            "default": "${yyyy-MM-dd}"
        },
        {
            "name": "min_price",
            "type": "single_input",
            "default": "100"
        },
        {
            "name": "product_ids",
            "type": "multi_input",
            "sep": ",",
            "wrapper": "'"
        }
    ],
    "visualization": [
        {
            "type": "python",
            "code": "# 返回表格\nresult = df"
        },
        {
            "type": "python",
            "code": "# 返回图\nimport plotly.express as px\nresult = px.bar(df.groupby(\"region\").sum().reset_index(), x=\"region\", y=\"quantity\")"
        }
    ]
}